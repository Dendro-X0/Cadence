{
  "$schema": "../../../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Cadence Desktop",
  "version": "0.1.0",
  "identifier": "com.cadence.app",
  "build": {
    "frontendDist": "../../web/dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm --prefix \"G:/Web Development Project/Codebases/cadence\" run dev:web",
    "beforeBuildCommand": "npm --prefix \"G:/Web Development Project/Codebases/cadence\" run build:web"
  },
  "app": {
    "windows": [
      {
        "title": "@cadence/desktop",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
